<Application x:Class="Eyeplayer.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <Window x:Key="windowMode" WindowState="Maximized" WindowStyle="None" Background="{x:Null}"/>
            <Window x:Key="windowMode_0" WindowState="Maximized" WindowStyle="None" Background="{DynamicResource imgMenuBg}"/>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Language/zh-TW.xaml" x:Name="LanguageResource"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
